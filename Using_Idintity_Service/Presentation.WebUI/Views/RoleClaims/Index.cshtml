@model List<string>
<div class="col-md-12 form-group">
    <a href="/RoleClaims/Create?RoleName=@ViewBag.roleName" class="btn btn-success">add claim</a>

</div>
<table class="table table-bordered table-striped table-info">
    <tr>
        <th>مقدار</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item</td>
            <td>
                <a href="/RoleClaims/RemoveClaim?roleName=@(ViewBag.roleName)&claimValue=@item" class="btn btn-danger">delete</a>
            </td>
        </tr>
    }
</table>
