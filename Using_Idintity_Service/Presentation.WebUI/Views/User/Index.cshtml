@model IEnumerable<UserDTO>
<div class="col-md-12 form-group">
    <a asp-action="Create" class="btn btn-success">Create</a>
</div>
<table class="table table-bordered table-striped table-info">
    <tr>
        <th>شناسه</th>
        <th>نام</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.UserName</td>
            <td>
                <a href="/UserClaims/Index?username=@item.UserName" class="btn btn-warning">claims</a>
                <a href="/UserRoles/Index?username=@item.UserName" class="btn btn-warning">roles</a>
                <a href="/User/RemoveUser?userName=@item.UserName" class="btn btn-danger">delete</a>
            </td>
        </tr>
    }
</table>
